{
    "inport": {
        "name": "inport",
        "label": "InPort",
        "description": "An input port for the graph",
        "icon": "sign-in",
        "driver": "GCCModelDriver",
        "args": "unused",
        "inports": [],
        "outports": [
            {"name": "value", "type": "all"}
        ]
    },
    "outport": {
        "name": "outport",
        "label": "OutPort",
        "description": "An output port for the graph",
        "icon": "sign-out",
        "driver": "GCCModelDriver",
        "args": "unused",
        "inports": [
            {"name": "value", "type": "all"}
        ],
        "outports": []
    },
    "canopy": {
        "name": "canopy",
        "label": "Canopy",
        "description": "Canopy Model",
        "icon": "umbrella",
        "driver": "GCCModelDriver",
        "args": "./src/canopy.cpp",
        "inports": [
            {"name": "plant_layout", "label": "plant_layout", "type": "all"},
            {"name": "init_canopy_structure", "label": "init_canopy_structure", "type": "all"},
            {"name": "growth_rate", "label": "growth_rate", "type": "all"},
            {"name": "time", "label": "time", "type": "all"}
        ],
        "outports": [
            {"name": "canopy_structure", "label": "canopy_structure", "type": "all"}
        ]
    },
    "growth": {
        "name": "growth",
        "label": "GrowthMat",
        "description": "Growth Model (MATLAB)",
        "icon": "pagelines",
        "driver": "MatlabModelDriver",
        "args": "./src/growth.m",
        "inports": [
            {"name": "photosynthesis_rate", "label": "photosynthesis_rate", "type": "all"}
        ],
        "outports": [
            {"name": "growth_rate", "label": "growth_rate", "type": "all"}
        ]
    },
    "growthpy": {
        "name": "growthpy",
        "label": "GrowthPy",
        "description": "Growth Model (Python)",
        "icon": "pagelines",
        "driver": "PythonModelDriver",
        "args": "./src/growth.py",
        "inports": [
            {"name": "photosynthesis_rate", "label": "photosynthesis_rate", "type": "all"}
        ],
        "outports": [
            {"name": "growth_rate", "label": "growth_rate", "type": "all"}
        ]
    },
    "light": {
        "name": "light",
        "label": "Light",
        "description": "Light Model",
        "icon": "sun-o",
        "driver": "GCCModelDriver",
        "args": [
            "./src/light.c",
            "-lm"
        ],
        "inports": [
            {"name": "ambient_light", "label": "ambient_light", "type": "all"},
            {"name": "canopy_structure", "label": "canopy_structure", "type": "all"}
        ],
        "outports": [
            {"name": "light_intensity", "label": "light_intensity", "type": "all"}
        ]
    },
    "photosynthesis": {
        "name": "photosynthesis",
        "label": "PhotoSyn",
        "description": "Photosynthesis Model",
        "icon": "exchange",
        "driver": "PythonModelDriver",
        "args": "./src/photosynthesis.py",
        "inports": [
            {"name": "light_intensity", "label": "light_intensity", "type": "all"},
            {"name": "temperature", "label": "temperature", "type": "all"},
            {"name": "co2", "label": "co2", "type": "all"}
        ],
        "outports": [
            {"name": "photosynthesis_rate", "label": "photosynthesis_rate", "type": "all"}
        ]
    },
    "grcm": {
        "name": "grcm",
        "label": "GrCM",
        "description": "Gene Expression Model",
        "icon": "intersex",
        "driver": "ModelDriver",
        "args": [
            "python",
            "./src/GrCM_plain.py"
        ],
        "inports": [
            {"name": "grcm_input1", "label": "GrCM_input1", "type": "all"},
            {"name": "grcm_static", "label": "GrCM_static", "type": "all"}
        ],
        "outports": [
            {"name": "grcm_output", "label": "GrCM_output", "type": "all"}
        ]
    },
    "mem": {
        "name": "mem",
        "label": "MeM",
        "description": "Metabolic Model",
        "icon": "cutlery",
        "driver": "MatlabModelDriver",
        "args": "./src/MeM_alone.m",
        "inports": [
            {"name": "mem_input1", "label": "MeM_input1", "type": "all"},
            {"name": "mem_input2", "label": "MeM_input2", "type": "all"},
            {"name": "mem_input3", "label": "MeM_input3", "type": "all"}
        ],
        "outports": [
            {"name": "mem_output", "label": "MeM_output", "type": "all", 
            "field_names": "PhotosynthesisRate", "field_units": "uMol/s/(m**2)"}
        ]
    },
    "rtr": {
        "name": "rtr",
        "label": "RTr",
        "description": "RayTracer Model",
        "icon": "lightbulb-o",
        "driver": "CMakeModelDriver",
        "args": [
            "cis_ray",
            "-a nofile",
            "-o {{ FOUT }}",
            "-f {{ START_HR }}",
            "-e {{ END_HR }}",
            "-i {{ STEP_HR }}",
            "-r {{ RAY_DIST }}",
            "-l {{ TRANSMITTANCE }}",
            "-j {{ REFLECTIVITY }}"
        ],
        "cmakeargs": "-DCISRUN:BOOL=ON",
        "inports": [
            {"name": "rtr_canopy_structure", "label": "rtr_canopy_structure", "type": "all"}
        ],
        "outports": [
            {"name": "rtr_environ", "label": "rtr_environ", "type": "all"}
        ]
    },
    "lem": {
        "name": "lem",
        "label": "LeM",
        "description": "Leaf Model",
        "icon": "leaf",
        "driver": "MakeModelDriver",
        "makedir": ".",
        "makefile": "./src/LeMMakeFile",
        "args": "LeM",
        "inports": [
            {"name": "lem_input1", "label": "LeM_input1", "type": "all"},
            {"name": "lem_static", "label": "LeM_static", "type": "all"},
            {"name": "lem_photo", "label": "LeM_photo", "type": "all"}
        ],
        "outports": [
            {"name": "lem_environ", "label": "LeM_environ", "type": "all", "field_names": "CO2,light,Temp,GRNC,GRNT"},
            {"name": "lem_output", "label": "LeM_output", "type": "all", "field_names": "LeafID,LeafTemperature,Ca,Ci,ANet,Transpiration,Radiation,SensibleHead,LatentHead,Emission,Me,Gs,PAR"}
        ]
    }
}

