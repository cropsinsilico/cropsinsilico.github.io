{
	"openapi": "3.0.0",
	"servers": [{
		"url": "https://api.orange.com/wifilocator/v2"
	}],
	"info": {
		"description": "This is a test",
		"version": "1.0.0",
		"title": "Test oauth2 password credentials"
	},
	"tags": [{
		"name": "hotspots",
		"description": "Test"
	}],
	"paths": {
		"/hotspots": {
			"get": {
				"consumes": [
					"application/json"
				],
				"produces": [
					"application/json"
				],
				"tags": [
					"hotspots"
				],
				"description": "Search for hotspots",
				"parameters": [{
					"name": "latitude",
					"description": "latitude",
					"in": "query",
					"required": true,
					"type": "number",
					"format": "float",
					"default": "48.8245"
				}, {
					"name": "longitude",
					"description": "longitude",
					"in": "query",
					"required": true,
					"type": "number",
					"format": "float",
					"default": "2.2743"
				}, {
					"name": "radius",
					"description": "radius",
					"in": "query",
					"required": true,
					"type": "integer",
					"format": "int32",
					"default": "1000"
				}],
				"responses": {
					"200": {
						"description": "Successful response"
					}
				},
				"security": [{
					"app_auth": []
				}]
			}
		}
	},
	"components": {
		"securitySchemes": {
			"user_auth": {
				"type": "oauth2",
				"flows": {
					"password": {
						"tokenUrl": "http://127.0.0.1:9002/oauth/v2/token"
					}
				}
			}
		}
	}
